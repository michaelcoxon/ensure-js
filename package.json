  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^2.2.48",
    "@types/prop-types": "^15.5.3",
    "@types/webpack-env": "^1.13.5",
    "awesome-typescript-loader": "^3.4.1",
    "chai": "^4.1.2",
    "dts-bundle": "^0.7.3",
    "file-loader": "^0.11.2",
    "json-loader": "^0.5.7",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "ts-node": "^4.0.2",
